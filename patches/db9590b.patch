+CREATE TABLE "new_SearchLog" (
+    "id" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
+    "termId" INTEGER,
+    "query" TEXT NOT NULL,
+    "language" TEXT NOT NULL DEFAULT 'es',
